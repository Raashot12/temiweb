{
  "name": "insurance-react-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --root src/",
    "api-generate": "curl https://insurance-api.plateaumed-dev.com/swagger/v1/swagger.json -o api.json && npx @rtk-query/codegen-openapi openapi-config.ts",
    "local-api-generate": "npx @rtk-query/codegen-openapi openapi-config.ts",
    "format": "prettier --ignore-path .gitignore write \"**/*.{ts,tsx,css,scss}\"",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/serialize": "^1.3.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@emotion/utils": "^1.4.2",
    "@hookform/resolvers": "^3.9.0",
    "@mantine/carousel": "^7.11.1",
    "@mantine/charts": "^7.11.1",
    "@mantine/core": "^7.11.0",
    "@mantine/dates": "^7.11.1",
    "@mantine/emotion": "^7.11.1",
    "@mantine/hooks": "^7.11.1",
    "@mantine/modals": "^7.11.1",
    "@mantine/notifications": "^7.11.1",
    "@reduxjs/toolkit": "^1.8.6",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@tabler/icons-react": "^3.8.0",
    "embla-carousel-autoplay": "^8.1.7",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "next": "14.2.4",
    "prettier": "^3.3.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.5.0",
    "react-redux": "^8.0.4",
    "react-toastify": "^10.0.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-redux": "^7.1.33",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-jest-dom": "^5.4.0",
    "jsdom": "^24.1.0",
    "msw": "^2.3.1",
    "ts-node": "^10.9.2",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,scss,css}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "eslintIgnore": [
    "./src/state/services/*.ts"
  ]
}
